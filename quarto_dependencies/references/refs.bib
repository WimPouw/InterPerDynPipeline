@article{laureOptimizationTransdiagnosticMobile2023,
  title = {Optimization of a {{Transdiagnostic Mobile Emotion Regulation Intervention}} for {{University Students}}: {{Protocol}} for a {{Microrandomized Trial}}},
  shorttitle = {Optimization of a {{Transdiagnostic Mobile Emotion Regulation Intervention}} for {{University Students}}},
  author = {Laure, Tajda and Engels, Rutger C. M. E. and Remmerswaal, Danielle and {Spruijt-Metz}, Donna and Konigorski, Stefan and Boffo, Marilisa},
  year = 2023,

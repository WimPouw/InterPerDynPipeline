@misc{piccininniCausalInferenceNof12024,
  title = {Causal Inference for {{N-of-1}} Trials},
  author = {Piccininni, Marco and Stensrud, Mats J. and Shahn, Zachary and Konigorski, Stefan},
  year = 2024,
  month = aug,
  number = {arXiv:2406.10360},

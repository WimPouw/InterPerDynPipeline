@article{tingEthnokinesiologyNeuromechanicalUnderstanding2024,
  title = {Ethnokinesiology: Towards a Neuromechanical Understanding of Cultural Differences in Movement},
  shorttitle = {Ethnokinesiology},
  author = {Ting, Lena H. and Gick, Bryan and Kesar, Trisha M. and Xu, Jing},
  year = 2024,
  month = aug,
  journal = {Philosophical Transactions of the Royal Society B: Biological Sciences},
  volume = {379},
  number = {1911},
  pages = {20230485},
  publisher = {Royal Society},
  doi = {10.1098/rstb.2023.0485},
  urldate = {2025-06-27},
  abstract = {Each individual's movements are sculpted by constant interactions between sensorimotor and sociocultural factors. A theoretical framework grounded in motor control mechanisms articulating how sociocultural and biological signals converge to shape movement is currently missing. Here, we propose a framework for the emerging field of ethnokinesiology aiming to provide a conceptual space and vocabulary to help bring together researchers at this intersection. We offer a first-level schema for generating and testing hypotheses about cultural differences in movement to bridge gaps between the rich observations of cross-cultural movement variations and neurophysiological and biomechanical accounts of movement. We explicitly dissociate two interacting feedback loops that determine culturally relevant movement: one governing sensorimotor tasks regulated by neural signals internal to the body, the other governing ecological tasks generated through actions in the environment producing ecological consequences. A key idea is the emergence of individual-specific and culturally influenced motor concepts in the nervous system, low-dimensional functional mappings between sensorimotor and ecological task spaces. Motor accents arise from perceived differences in motor concept topologies across cultural contexts. We apply the framework to three examples: speech, gait and grasp. Finally, we discuss how ethnokinesiological studies may inform personalized motor skill training and rehabilitation, and challenges moving forward. This article is part of the theme issue `Minds in movement: embodied cognition in the age of artificial intelligence'.},
  keywords = {biomechanics,embodiment,individual differences,motor accent,motor concept,movement diversity},
  file = {C:\Users\Admin\Zotero\storage\LDW5K6CR\Ting et al. - 2024 - Ethnokinesiology towards a neuromechanical unders.pdf}
}

@book{tomaselloBecomingHumanTheory2019,

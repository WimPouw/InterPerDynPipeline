}

@inproceedings{vaishnaviHumanActivityLower2025,
  title = {Human Activity for the Lower Limb Pose Estimation Analyzed Using Various Marker-Less Deep Learning Techniques and Kinovea},
  booktitle = {2025 {{International Conference}} on {{Sensors}} and {{Related Networks}} ({{SENNET}}) {{Special Focus}} on {{Digital Healthcare}}(64220)},
  author = {Vaishnavi, K. and Pavithra, L. K.},
  year = 2025,
  month = jul,
  pages = {1--6},
  doi = {10.1109/SENNET64220.2025.11136035},
  urldate = {2025-11-19},
  abstract = {Human pose estimation (HPE) from monocular RGB images has been experiencing a rapid development in the research areas of computer vision and has the benefit of being tested in several areas of application such as medicine, sports training, virtual reality, surveillance, etc. Keypoint detection of HPE is subject to many complications such as occlusion and environments with environmental background. As a solution to some of these problems, they rely heavily on marker-based models to assess human poses which rely on reflective markers and quality cameras. Regardless of quality, it will always be limited by the disruption of natural motion that reflective markers cause due to misplacement. Most recently, markerless keypoint detection models have been developed and implemented in HPE. In this research, markerless models, such as MoveNet, MediaPipe, YOLOv8, Keypoint R-CNN, and PoseNet will be evaluated by the performance of their evaluation keys across three datasets. Ground truth keypoints for these datasets were generated using a tool called KINOVEA, with manual joint position annotations serving as reference images for evaluating the five deep learning pose estimation models across three subjects performing various activities. Key evaluation metrics include PDJ, PCK, PCP, AR, MSE, RMSE, AP, and mAP. MoveNet and YOLOv8 demonstrated the best overall performance. During the ramp walk activity, MoveNet achieved the lowest PDJ (0.054) and the highest PCK (0.054), indicating strong joint localization. MediaPipe and PoseNet performed well in terms of AR for car and ramp walk activities. YOLOv8 achieved the lowest MSE (0.009) and RMSE (0.095) along with high AP and mAP values. In summary, MoveNet excels in joint localization, while YOLOv8 provides robust and efficient pose estimation across diverse scenarios.},
  keywords = {Analytical models,Biological system modeling,Biomedical imaging,Deep learning,Human pose estimation,keypoint rcnn,kinovea software,Legged locomotion,Location awareness,Marker less human motion analysis,mediapipe,Motion analysis,Movenet,Pose estimation,posenet,Real-time systems,Solid modeling,yolov8},
  file = {C:\Users\Admin\Zotero\storage\TTLNXZH9\11136035.html}
}

@article{vanderwelUnderstandingJointAction2021,
  title = {Understanding Joint Action: {{Current}} Theoretical and Empirical Approaches},

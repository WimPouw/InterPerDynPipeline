@article{haleAreYouMy2020,
  title = {Are {{You}} on {{My Wavelength}}? {{Interpersonal Coordination}} in {{Dyadic Conversations}}},
  shorttitle = {Are {{You}} on {{My Wavelength}}?},
  author = {Hale, Joanna and Ward, Jamie A. and Buccheri, Francesco and Oliver, Dominic and Hamilton, Antonia F. de C.},
  year = 2020,
  month = mar,
  journal = {Journal of Nonverbal Behavior},

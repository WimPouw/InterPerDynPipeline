@article{lozano-goupilAutomaticQuantificationHand2025,
  title = {Automatic Quantification of Hand Gestures in Current and Remitted {{Major Depressive Disorder}} during Oral Expression},
  author = {{Lozano-Goupil}, Juliette and Shankman, Stewart A. and Walther, Sebastian and Wuethrich, Florian and Maher, Riley E. and Grzelak, Lauren N. and Mittal, Vijay A.},
  year = 2025,

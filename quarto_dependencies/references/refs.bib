  langid = {english},
  keywords = {convergent cross-mapping,early warnings,empirical dynamic modelling,gaussian process,S-map,scenario exploration,simplex,stability},
  file = {C:\Users\Admin\Zotero\storage\WUTP4Y7F\2041-210X.html}
}

